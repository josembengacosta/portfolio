            "url": "https://jmbenga.com/manifest.json"
        }
    ],
    "prefer_related_applications": false,
    "launch_handler": {
        "client_mode": ["navigate-existing", "auto"]
    },
    "protocol_handlers": [
        {
            "protocol": "web+jmbenga",
            "url": "/?protocol=%s"
        }
    ],
    "edge_side_panel": {
        "preferred_width": 400
    },
    "display_override": ["window-controls-overlay", "standalone"],
    "handle_links": "preferred",
    "share_target": {
        "action": "/share-target/",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "file_handlers": [
        {
            "action": "/open-file/",
            "accept": {
                "text/markdown": [".md"],
                "application/json": [".json"],
                "text/html": [".html"]
            }
        }
    ],
    "capture_links": "new-client"
